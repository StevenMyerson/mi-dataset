[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/StevenMyerson
[access "refs/for/refs/*"]
	read = group user/StevenMyerson
	push = group user/StevenMyerson
	label-Code-Review = -2..+2 group user/StevenMyerson
	label-Verified = -1..+1 group user/StevenMyerson
	rebase = group user/StevenMyerson
	removeReviewer = group user/StevenMyerson
	submit = group user/StevenMyerson
[access "refs/heads/*"]
	read = group user/StevenMyerson
	create = group user/StevenMyerson
	push = +force group user/StevenMyerson
	pushMerge = group user/StevenMyerson
[access "refs/tags/*"]
	push = +force group user/StevenMyerson
	pushTag = group user/StevenMyerson
	pushSignedTag = group user/StevenMyerson
